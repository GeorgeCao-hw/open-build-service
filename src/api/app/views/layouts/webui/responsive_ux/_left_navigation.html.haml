#left-navigation
  - if User.session
    = render partial: 'layouts/webui/responsive_ux/places', locals: { navigation: :left }
    - if content_for(:actions)
      = render partial: 'layouts/webui/responsive_ux/actions', locals: { navigation: :left }
  - else
    = render partial: 'layouts/webui/responsive_ux/left_navigation_nobody'

